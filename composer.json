{
    "name": "semhoun/slim-skeleton-mvc",
    "description": "Simple Slim Framework 4 skeleton with Twig, Monolog, Doctrine in Sqlite.",
    "license": "MIT",
    "type": "project",
    "keywords": ["slim-framework", "skeleton", "mvc", "sqlite", "twig", "monolog", "doctrine", "console"],
    "authors": [
        {
            "name": "Nathanael Semhoun",
            "email": "nathanael@semhoun.net",
            "homepage": "http://www.semhoun.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\Controller\\": "src/Controller",
            "App\\Entity\\": "src/Entity",
            "App\\Command\\" : "src/Command",
            "App\\Middleware\\" : "src/Middleware",
            "App\\Renderer\\" : "src/Renderer",
            "App\\Model\\" : "src/Model",
            "App\\Service\\" : "src/Service"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "require": {
        "php": ">=7.1|^8.0",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "php-di/php-di": "^6.3",
        "slim/twig-view": "^3.3",
        "monolog/monolog": "^2.0",
        "symfony/console": "^6.0",
        "semhoun/proxy-detection-middleware": "^0.5.0",
        "symfony/cache": "^6.0",
        "semhoun/runtracy": "^0.2.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    }
}
